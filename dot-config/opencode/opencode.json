{
  "$schema": "https://opencode.ai/config.json",
  "mcp": {
    "mongodb": {
      "type": "local",
      "enabled": false,
      "command": ["fnm", "exec", "--using=v24.13.0", "npx", "-y", "mongodb-mcp-server@latest", "--readOnly"],
      "environment": {
        "MDB_MCP_CONNECTION_STRING": "{env:MONGODB_URI}"
      }
    }
  },
  "provider": {
    "ollama": {
      "models": {
        "qwen2.5-coder:32b": {
          "_launch": true,
          "name": "qwen2.5-coder:32b"
        },
        "qwen3-coder:30b": {
          "_launch": true,
          "name": "qwen3-coder:30b"
        },
        "qwen3:8b": {
          "_launch": true,
          "name": "qwen3:8b"
        }
      },
      "name": "Ollama (local)",
      "npm": "@ai-sdk/openai-compatible",
      "options": {
        "baseURL": "http://127.0.0.1:11434/v1"
      }
    }
  }
}

